->\n\n<!DOCTYPE html>\n<html>\n<head>\n    <!-- head content -->\n</head>\n<body>\n    <div class=\"page\">\n        <div class=\"header\">\n            <h1>Ticket Booked - {{ doc.name }}</h1>\n        </div>\n        <div class=\"content\">\n            <!-- Customer Information -->\n            <div class=\"section\">\n                <h2>Customer Information</h2>\n                <p><strong>Customer Name:</strong> {{ doc.customer_name }}</p>\n                <p><strong>Contact Email:</strong> {{ doc.email_address }}</p>\n                <!-- Add more customer information fields as needed -->\n            </div>\n            <!-- Event Details -->\n            <div class=\"section\">\n                <h2>Event Details</h2>\n                <p><strong>Event Name:</strong> {{ doc.event_name }}</p>\n                <p><strong>Number of Tickets:</strong> {{ doc.number_of_tickets }}</p>\n                <p><strong>Ticket Price:</strong> {{ doc.ticket_price }}</p>\n                <!-- Add more event details fields as needed -->\n            </div>\n            <!-- Booking Details -->\n            <div class=\"section\">\n                <h2>Booking Details</h2>\n                <p><strong>Total Amount:</strong> {{ doc.total_amount }}</p>\n                <p><strong>Booking Date:</strong> {{ doc.booking_date }}</p>\n                <p><strong>Status:</strong> {{ doc.payment_status }}</p>\n                <!-- Add more booking details fields as needed -->\n            </div>\n            <!-- Booked Tickets Child -->\n            <div class=\"section\">\n                <h2>Booked Tickets</h2>\n                <table>\n                    <thead>\n                        <tr>\n                            <th>Event Name</th>\n                            <th>Date</th>\n                            <!-- Add more headers if needed -->\n                        </tr>\n                    </thead>\n                    <tbody>\n                        {% for ticket in membership %}\n                            <tr>\n                                <td>{{ ticket.event_name }}</td>\n                                <td>{{ ticket.date }}</td>\n                                <!-- Display more fields from the child table -->\n                            </tr>\n                        {% endfor %}\n                    </tbody>\n                </table>\n            </div>\n        </div>\n    </div>\n</body>\n</html>\n\n",
